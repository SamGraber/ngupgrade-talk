{
  "name": "ngupgrade-talk",
  "version": "1.0.0",
  "description": "Upgrade an angular 1 application to angular 2",
  "main": "app.js",
  "scripts": {
    "json-server": "json-server --watch data.json",
    "lite-server": "lite-server -c config/lite-server.config.js",
    "tsc": "tsc",
    "tsc.watch": "tsc -w",
    "start": "npm-run-all --parallel tsc.watch json-server lite-server",
    "test": "karma start config/karma.conf.js"
  },
  "author": "Sam Graber <sgraber@renovo1.com>",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.6.2",
    "angular-route": "^1.6.2",
    "bootstrap": "^3.3.7",
    "json-server": "^0.9.4",
    "lite-server": "^2.2.2",
    "npm-run-all": "^4.0.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.2",
    "chai": "^3.5.0",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "sinon": "^1.17.7",
    "typescript": "^2.1.6"
  }
}
